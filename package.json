{
  "homepage": ".",
  "name": "microservice-tcp-nbc",
  "version": "1.0.0",
  "description": "template for microservice ",
  "private": true,
  "scripts": {
    "test": "cross-env NODE_ENV=test jest   --forceExit ",
    "start:gate": "babel-node src/server/app.js",
    "start:dis": " babel-node src/server/modules/distributor.js",
    "start:users": "babel-node src/server/modules/users/users.module.js",
    "start:posts": " babel-node src/server/modules/posts/posts.module.js",
    "start:admin": "babel-node src/server/modules/admin/admin.module.js",
    "start:threads": " babel-node src/server/modules/threads/threads.module.js",
    "start:chat": " babel-node src/server/modules/chat/chat.module.js",
    "start:gate_build": "node dist/server/app.js",
    "start:dis_build": "  node dist/server/modules/distributor.js",
    "start:users_build": "  node dist/server/modules/users/users.module.js",
    "start:posts_build": " node dist/server/modules/posts/posts.module.js",
    "start:admin_build": " node dist/server/modules/admin/admin.module.js",
    "start:threads_build": " node dist/server/modules/threads/threads.module.js",
    "start:chat_build": " node dist/server/modules/chat/chat.module.js",
    "start": "concurrently \"npm run start:gate\" \"npm run start:dis\" \"npm run start:users\" \"npm run start:posts\"  \"npm run start:admin\"  \"npm run start:threads\"  \"npm run start:chat\"",
    "start_build": "concurrently \"npm run start:gate_build\" \"npm run start:dis_build\" \"npm run start:users_build\" \"npm run start:posts_build\"  \"npm run start:admin_build\"  \"npm run start:threads_build\"  \"npm run start:chat_build\"",
    "build": " concurrently \"babel src --out-dir dist --copy-files\" \"npx webpack\" "
  },
